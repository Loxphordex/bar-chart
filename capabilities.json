{
    "dataRoles": [{
            "displayName": "Axis",
            "name": "category",
            "kind": "Grouping"
        },
        {
            "displayName": "Values",
            "name": "measure",
            "kind": "Measure"
        },
        {
            "displayName": "Order",
            "name": "order",
            "kind": "Measure"
        }
    ],
    "objects": {},
    "dataViewMappings": [{
        "conditions": [{
                "category": {
                    "max": 1
                }
            },
            {
                "measure": {
                    "max": 1
                }
            }
        ],
        "categorical": {
            "categories": {
                "for": {
                    "in": "category"
                },
                "dataReductionAlgorithm": {
                    "top": {}
                }
            },
            "values": {
                "select": [{
                    "bind": {
                        "to": "measure"
                    }
                }]
            }
        }
    }],
    "sorting": {
        "implicit": {
            "clauses": [
                {
                    "role": "order",
                    "direction": 1
                }
            ]
        }
    }
}